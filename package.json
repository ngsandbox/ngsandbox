{
  "name": "ngsandbox",
  "version": "0.1.0",
  "description": "Angular Sandbox",
  "main": "server.js",
  "author": "VMA",
  "license": "MIT",
  "scripts": {
    "startNode": "tsc && \"npm run copy:node\" && concurrently \"npm run node\" ",
    "start": "tsc && concurrently \"npm run copy:node\" \"npm run tsc:w\" \"npm run lite:jit\" ",
    "node": "node ./server/server.js",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "lite:jit": "lite-server -c bs-config.json",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "copy:node": "node ./copy-dist-files.js",
    "explorer:aot": "source-map-explorer aot/dist/build.js",
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test:once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/compiler-cli": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/platform-server": "2.2.3",
    "@angular/router": "3.2.3",
    "@angular/upgrade": "2.2.3",
    "@types/lodash": "4.14.40",
    "@types/mongoose": "4.6.1",
    "@types/node": "6.0.51",
    "angular2-in-memory-web-api": "0.0.21",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "hammer-timejs": "1.1.0",
    "hammerjs": "2.0.8",
    "jquery": "3.1.1",
    "jquery.cookie": "1.4.1",
    "lodash": "4.17.2",
    "mongoose": "4.7.0",
    "reflect-metadata": "0.1.8",
    "rollup": "0.36.4",
    "rxjs": "5.0.0-rc.4",
