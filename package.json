{
  "name": "ngsandbox",
  "version": "0.1.0",
  "description": "Angular Sandbox",
  "main": "server.js",
  "author": "VMA",
  "license": "MIT",
  "scripts": {
    "startNode": "tsc && \"npm run copy:node\" && concurrently \"npm run node\" ",
    "start": "tsc && concurrently \"npm run copy:node\" \"npm run tsc:w\" \"npm run lite:jit\" ",
    "node": "node ./server/server.js",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "lite:jit": "lite-server -c bs-config.json",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "copy:node": "node ./copy-dist-files.js",
    "explorer:aot": "source-map-explorer aot/dist/build.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "@angular/common": "2.2.0",
