{
  "name": "ngsandbox",
  "version": "0.1.0",
  "description": "Angular Sandbox",
  "main": "server.js",
  "author": "VMA",
  "license": "MIT",
  "scripts": {
    "startNode": "tsc && gulp && concurrently \"npm run node\" ",
    "start": "tsc && gulp && concurrently \"npm run tsc:w\" \"npm run lite:jit\" ",
    "node": "node ./server/server.js",
    "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
    "lite:jit": "lite-server -c bs-config.json",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "copy:aot": "node copy-dist-files",
    "explorer:aot": "source-map-explorer aot/dist/build.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "dependencies": {
    "@angular/common": "2.1.2",
    "@angular/compiler": "2.1.2",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/http": "2.1.2",
    "@angular/platform-browser": "2.1.2",
    "@angular/platform-browser-dynamic": "2.1.2",
    "@angular/platform-server": "^2.1.2",
    "@angular/router": "3.1.2",
    "@angular/upgrade": "2.1.2",
    "angular2-in-memory-web-api": "0.0.21",
    "body-parser": "^1.15.2",
    "core-js": "^2.4.1",
    "express": "~4.14.0",
    "hammerjs": "^2.0.8",
    "jquery.cookie": "^1.4.1",
    "lodash": "^4.16.6",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.2",
    "systemjs": "0.19.40",
    "typescript": "^2.0.7",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/core-js": "^0.9.34",
    "@types/express": "^4.0.33",
    "@types/express-serve-static-core": "^4.0.39",
    "@types/jasmine": "^2.5.37",
    "@types/jquery.cookie": "^1.4.28",
    "@types/lodash": "^4.14.38",
    "@types/mime": "0.0.29",
    "@types/mongoose": "^4.5.40",
    "@types/node": "^6.0.46",
    "@types/serve-static": "^1.7.31",
    "concurrently": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-rimraf": "^0.2.1",
    "lite-server": "^2.2.2",
    "merge": "^1.2.0",
    "mongoose": "^4.6.6",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.3",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "source-map-explorer": "^1.3.3"
  }
}
