    "express": "~4.14.0",
    "firebase": "3.6.1",
    "hammerjs": "2.0.8",
    "jquery": "3.1.1",
    "jquery.cookie": "1.4.1",
    "lodash": "4.17.2",
    "modernizr": "3.3.1",
